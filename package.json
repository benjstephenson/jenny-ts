  "author": "Benji Stephenson",
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjstephenson/jenny-ts.git"
  },
  "homepage": "https://github.com/benjstephenson/jenny-ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "build": "tsc -p .",
    "test": "jest"
